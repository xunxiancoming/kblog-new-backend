{
  "name": "kblog-backend",
  "version": "1.0.0",
  "description": "博客后端系统",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:admin\"",
    "dev:api": "cd api && npm run start:dev",
    "dev:admin": "cd admin && npm run dev",
    "build": "npm run build:api && npm run build:admin",
    "build:api": "cd api && npm run build",
    "build:admin": "cd admin && npm run build",
    "install:all": "npm install && cd api && npm install && cd ../admin && npm install"
  },
  "workspaces": [
    "./api",
    "./admin"
  ],
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "keywords": ["blog", "nestjs", "react", "typescript"],
  "author": "",
  "license": "ISC"
}